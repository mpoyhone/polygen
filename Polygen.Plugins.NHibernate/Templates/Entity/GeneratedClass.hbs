{{#each Model.NamespaceImports}}
using {{this}};
{{/each}}

namespace {{Model.ClassNamespace}}
{
    /// <summary>
    /// {{Model.Documentation}}
    /// </summary>
    {{Model.ModifiersString}} class {{Model.ClassName}}
    {
        {{#Model.Fields}}
            {{> NHibernate/Entity/Field}}
        {{/Model.Fields}}

        {{#Model.Properties}}
            {{> NHibernate/Entity/Property}}
        {{/Model.Properties}}
    }
}
